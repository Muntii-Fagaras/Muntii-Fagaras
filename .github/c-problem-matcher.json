{
  "problemMatcher": [
    {
      "owner": "gui-base",  
      "pattern": [
        {
          "regexp": "^\\s*(info|warning|error)\\s•\\s(.*)\\s•\\s(.*):(\\d+):(\\d+)\\s•\\s(.*)$",
          "message": 4,  
          "file": 1,
          "fromPath": 1,
          "line": 2,
          "column": 3
        }
      ]
    }
  ]
}
